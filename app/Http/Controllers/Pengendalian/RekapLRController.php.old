    {
        $rkap_bulan = array('', 'RKAP Jan', 'RKAP Feb', 'RKAP Mar', 'RKAP Apr', 'RKAP May', 'RKAP Jun', 'RKAP Jul', 'RKAP Aug', 'RKAP Sep', 'RKAP Oct', 'RKAP Nov', 'RKAP Dec');
